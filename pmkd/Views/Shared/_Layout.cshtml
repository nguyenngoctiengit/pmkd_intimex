        $(document).ready(function () {
            $(function () {
                var start = moment("2021/01/01");
                var end = moment("2021/12/30");

                function cb(start, end) {
                    $('#rangecontact span').html(start.format('MM/DD/YYYY') + ' - ' + end.format('MM/DD/YYYY'));
                }

                $('#rangecontact').daterangepicker({
                    autoUpdateInput: true,
                    locale: {
                        cancelLabel: 'Clear'
                    },
                    startDate: start,
                    endDate: end,
                    ranges: {
                        'Today': [moment(), moment()],
                        'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],
                        'Last 7 Days': [moment().subtract(6, 'days'), moment()],
                        'Last 30 Days': [moment().subtract(29, 'days'), moment()],
                        'This Month': [moment().startOf('month'), moment().endOf('month')],
                        'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')]
                    }
                }, cb);

                cb(start, end);

            });
            $('#rangecontact').on('cancel.daterangepicker', function (ev, picker) {
                $(this).val('');
            });

            $('#rangecontact').on('apply.daterangepicker', function (ev, picker) {
                var start = picker.startDate;
                var end = picker.endDate;
                $.fn.dataTable.ext.search.push(
                    function (settings, data, dataIndex) {
                        var min = start;
                        var max = end;
                        var startDate = new Date(data[5]);
                        if (min == null && max == null) {
                            return true;
                        }
                        if (min == null && startDate <= max) {
                            return true;
                        }
                        if (max == null && startDate >= min) {
                            return true;
                        }
                        if (startDate <= max && startDate >= min) {
                            return true;
                        }
                        return false;
                    }
                );
                otable.draw();
                $.fn.dataTable.ext.search.pop();
            });
        });
