                                chatluongbao.value = chuoi[j].Quanlitybag;
                                sobao.value = chuoi[j].SoBao;
                                tlbaobi.value = chuoi[j].TlBao;
                                makhach.value = chuoi[j].CustCode;
                                tenkhach.value = chuoi[j].CustName;
                                systemId.value = chuoi[j].SystemId;
                                tlin.value = chuoi[j].TlIn;
                                if (chuoi[j].Status == 2) {
                                    $("#trangthai").text("MẪU ĐẠT THỬ NẾM")
                                } else if (chuoi[j].Status == 3) {
                                    $("#trangthai").text("NHẬP CHẠY HÀNG THƯỜNG")
                                } else if (chuoi[j].Aprove == 0) {
                                    $("#trangthai").text("MẪU KHÔNG ĐẠT")
                                }
                                if (chuoi[j].CachCan == 0) {
                                    $("#cachcan").val("0");
                                } else if (chuoi[j].CachCan == 1) {
                                    $("#cachcan").val("1");
                                }
                                if (chuoi[j].HinhThucCan == 0) {
                                    $("#loaican").val("0");
                                } else if (chuoi[j].HinhThucCan == 1) {
                                    $("#loaican").val("1");
                                } else if (chuoi[j].HinhThucCan == 2) {
                                    $("#loaican").val("2");
                                }
                            }
                        }
                    }
                }
            })
        }
